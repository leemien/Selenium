<?xml version="1.0" encoding="UTF-8"?>
<configuration status="OFF">
    <appenders>
	    <Console name="Console" target="SYSTEM_OUT">
		    <PatternLayout charset="UTF-8" pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} [%t] %-5level %l - %msg%n"/>
		</Console>
	</appenders>
    <loggers>
	    <root level="INFO">
		    <appender-ref ref="Console"/>
		</root>
	</loggers>
</configuration>